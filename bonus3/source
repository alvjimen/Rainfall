
int main(int argc, char *argv[])
{
   FILE *fptr;             // fptr [esp + 0x9c]
   char buffer[124] = {0}; // buffer [esp + 0x18]

   fptr = fopen("/home/user/end/.pass", "r");
   if (!fptr || argc != 2)
      return (-1)
   fread(buffer, 1, 66, fptr);
   buffer[9] = '\0';
   buffer[atoi(argv[1])] = '\0';
   fread(buffer + 66, 1, 41, fptr);
   fclose(fptr);
   if (!strcmp(buffer, argv[1]))
   {
    puts(buffer + 42);
    return (0);
   }
   execl('/bin/sh', 'sh', NULL);
   return (0);
}