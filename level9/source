class N {
 private:
  char _buffer[100];
  int  _value;
 public:

  void setAnnotation(char* str) {
    // Unsafe copy without length checking
    size_t len = strlen(str);
    memcpy(annotation, str, len);
  }

  N(int val) {
   this->_value = val;
  }

  virtual int operator+(N& other) {
    // Adds the value at offset 0x68 of both objects
    return this->value + other.value;
  }

    // Overloaded - operator
  virtual int operator-(N& other) {
    return this->value - other.value;
  }
  
  void setAnnotation(char *str) {
    size_t len = strlen(str);
    strcpy(_buffer, str, len);
  }
};

int main(int argc, char **argv, char **envp)
{
  if (argc == 1)
    _exit(1);
  N *a = new N(5);
  N *b = new N(6);
  b->setAnnotation(envp[1]);
  return ((int)b+a);
}
